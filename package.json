  "pre-commit": [
    "lint.staged"
  ],
  "dependencies": {
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "cac": "^6.7.14",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
